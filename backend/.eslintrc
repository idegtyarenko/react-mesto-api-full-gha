{
  "extends": "airbnb-base",
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "import/extensions": [
          "error",
          {
              "js": "ignorePackages"
          }
        ]
      }
    }
  ]
}